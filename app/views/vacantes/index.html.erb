<div class="wrapper">
  <p id="notice"><%= notice %></p>

  <h4 class="center">Mis Vacantes</h4>

  <table>
    <thead>
      <tr>
        <th>Titulo</th>
        <th>Salario</th>
        <th>Descripci√≥n</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody class="tabla-fea2">
      <% @vacantes.each do |vacante| %>
        <tr>
          <td><%= vacante.title %></td>
          <td><%= vacante.salary %></td>
          <td><%= vacante.description %></td>
          <td><%= link_to 'Ver', vacante %></td>
          <td><%= link_to 'Editar', edit_vacante_path(vacante) %></td>
          <td><%= link_to 'Eliminar', vacante, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>